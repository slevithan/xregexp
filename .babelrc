{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
      },
    ],
    "add-module-exports",
    "transform-xregexp",
    "array-includes"
  ],
  "presets": [
    [
      "@babel/env", {
        "exclude": [
          "transform-literals",
          "transform-sticky-regex"
        ]
      }
    ]
  ]
}
